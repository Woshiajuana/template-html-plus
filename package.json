{
  "name": "template-multi-html",
  "version": "1.0.0",
  "description": "webpack building multiple page template",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.config.js",
    "build:dev": "rm -rf ./dist/* && cross-env NODE_ENV=development webpack --config build/webpack.config.js",
    "build:prod": "rm -rf ./dist/* && cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
